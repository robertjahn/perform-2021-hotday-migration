{
  "type": "SERVICE",
  "nameFormat": "{DockerContainerGroupInstance:ContainerName}",
  "displayName": "{{ .name }}",
  "enabled": true,
  "rules": [
    {
      "key": {
        "attribute": "DOCKER_CONTAINER_NAME"
      },
      "comparisonInfo": {
        "type": "STRING",
        "operator": "EXISTS",
        "value": null,
        "negate": false,
        "caseSensitive": null
      }
    },
    {
      "key": {
        "attribute": "PROCESS_GROUP_PREDEFINED_METADATA",
        "dynamicKey": "KUBERNETES_NAMESPACE",
        "type": "PROCESS_PREDEFINED_METADATA_KEY"
      },
      "comparisonInfo": {
        "type": "STRING",
        "operator": "EXISTS",
        "value": null,
        "negate": true,
        "caseSensitive": null
      }
    }
  ]
}